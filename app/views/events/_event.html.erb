<div class="event-box">
  <div class="event-photo">
    <%= image_tag event.photo.url(:strip) %>
  </div>
  <div class="event-name">
    <h2><%= link_to event.name, event_path(event) %></h2>
      <div id='event-info'>
        <h3><%= event.artist %></h3>
        <p><%= event.venue %></p>
        <p><%= event.date %></p>
      </div>
  </div>
</div>